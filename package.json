{
  "name": "geography",
  "version": "0.3.0",
  "description": "Geography Game",
  "main": "index.js",
  "repository": "git@gitlab.com:yellowberry/geography",
  "author": "Dmitriy Pushkov <ezze@ezze.org>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "build": "yarn copy:cesium && yarn copy:challenges && webpack --mode=production --progress",
    "build:dev": "yarn copy:cesium && yarn copy:challenges && webpack --mode=development --progress",
    "wds": "webpack-dev-server --mode=production --progress",
    "wds:dev": "webpack-dev-server --mode=development --progress",
    "copy:cesium": "copyfiles -s -u 4 \"node_modules/cesium/Build/Cesium/**/*\" dist/cesium",
    "copy:challenges": "rm -Rf dist/challenges && copyfiles -u 1 \"src/challenges/**/*\" dist",
    "lint": "eslint src"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "bowser": "^2.11.0",
    "bulma": "^0.9.1",
    "cesium": "^1.74.0",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "i18next": "^19.8.1",
    "koa": "^2.13.0",
    "koa-static": "^5.0.0",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "mobx-utils": "^5.6.1",
    "moment": "^2.29.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.3",
    "react-loadable": "^5.5.0",
    "react-loading": "^2.0.3",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/register": "^7.11.5",
    "autoprefixer": "^10.0.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-mobx": "^2.0.0",
    "copyfiles": "^2.4.0",
    "css-loader": "^4.3.0",
    "eslint": "^7.10.0",
    "eslint-config-ezze-react": "^0.9.0",
    "eslint-plugin-react": "^7.21.3",
    "favicons-webpack-plugin": "^2.1.0",
    "file-loader": "^6.1.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "mini-css-extract-plugin": "^0.11.3",
    "node-sass": "^4.14.1",
    "postcss": "^8.1.1",
    "postcss-loader": "^4.0.3",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.3.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
