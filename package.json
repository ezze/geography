{
  "name": "geography",
  "version": "0.1.0",
  "description": "Geography game",
  "main": "index.js",
  "repository": "git@gitlab.com:yellowberry/geography",
  "author": "Dmitriy Pushkov <ezze@ezze.org>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "build": "yarn copy:cesium && yarn copy:challenges && webpack --mode=production --progress",
    "build:dev": "yarn copy:cesium && yarn copy:challenges && webpack --mode=development --progress",
    "wds": "webpack-dev-server --mode=production --progress",
    "wds:dev": "webpack-dev-server --mode=development --progress",
    "copy:cesium": "copyfiles -s -u 4 \"node_modules/cesium/Build/Cesium/**/*\" dist/cesium",
    "copy:challenges": "rm -Rf dist/challenges && copyfiles -u 1 \"src/challenges/**/*\" dist",
    "lint": "eslint src"
  },
  "dependencies": {
    "bulma": "^0.7.5",
    "cesium": "^1.59.0",
    "classnames": "^2.2.6",
    "core-js": "^3.1.4",
    "i18next": "^17.0.6",
    "koa": "^2.7.0",
    "koa-static": "^5.0.0",
    "mobx": "^5.13.0",
    "mobx-react": "^6.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-i18next": "^10.11.4",
    "react-loadable": "^5.5.0",
    "react-loading": "^2.0.3",
    "react-router-dom": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-preset-mobx": "^2.0.0",
    "css-loader": "^3.1.0",
    "eslint": "^6.1.0",
    "eslint-config-ezze-react": "^0.8.0",
    "eslint-plugin-react": "^7.14.3",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
