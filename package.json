{
  "name": "geography",
  "version": "0.3.0",
  "description": "Geography Game",
  "main": "index.js",
  "repository": "git@gitlab.com:yellowberry/geography",
  "author": "Dmitriy Pushkov <ezze@ezze.org>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "build": "yarn copy:cesium && yarn copy:challenges && webpack --mode=production --progress",
    "build:dev": "yarn copy:cesium && yarn copy:challenges && webpack --mode=development --progress",
    "wds": "webpack-dev-server --mode=production --progress",
    "wds:dev": "webpack-dev-server --mode=development --progress",
    "copy:cesium": "copyfiles -s -u 4 \"node_modules/cesium/Build/Cesium/**/*\" dist/cesium",
    "copy:challenges": "rm -Rf dist/challenges && copyfiles -u 1 \"src/challenges/**/*\" dist",
    "lint": "eslint src"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "bowser": "^2.9.0",
    "bulma": "^0.8.2",
    "cesium": "^1.69.0",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "i18next": "^19.4.4",
    "koa": "^2.12.0",
    "koa-static": "^5.0.0",
    "mobx": "^5.15.4",
    "mobx-react": "^6.2.2",
    "mobx-utils": "^5.5.7",
    "moment": "^2.26.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.4.0",
    "react-loadable": "^5.5.0",
    "react-loading": "^2.0.3",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "autoprefixer": "^9.8.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-mobx": "^2.0.0",
    "copyfiles": "^2.2.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.0.0",
    "eslint-config-ezze-react": "^0.9.0",
    "eslint-plugin-react": "^7.20.0",
    "favicons-webpack-plugin": "^2.1.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
